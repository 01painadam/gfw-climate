#peatland_drainage{
  polygon-fill: #ECF0F6;
  polygon-opacity: 1;
  line-color: #FFF;
  line-width: 0.1;
  line-opacity: 1;
}
#peatland_drainage [ emisc02ha <= 73] {
   polygon-fill: #0F3B82;
}
#peatland_drainage [ emisc02ha <= 55] {
   polygon-fill: #6182B5;
}
#peatland_drainage [ emisc02ha <= 40] {
   polygon-fill: #B2C2DB;
}
#peatland_drainage [ emisc02ha <= 0] {
   polygon-fill: #ECF0F6;
}